{
    "cleanUrls": true,
    "routes": [
        {
            "src": "/assets/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "continue": true
        },
        {
            "src": "/(.*)",
            "headers": {
                "Cross-Origin-Opener-Policy": "same-origin-allow-popups",
                "Cross-Origin-Embedder-Policy": "credentialless",
                "X-Content-Type-Options": "nosniff"
            },
            "continue": true
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}